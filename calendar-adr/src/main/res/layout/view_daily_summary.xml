<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/cs_common_match"
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:paddingBottom="@dimen/margin_2dp">

    <LinearLayout
        android:id="@+id/lnr_id_no_data"
        style="@style/cs_common_match"
        android:gravity="center">

        <TextView
            style="@style/wl_common_text"
            android:gravity="center"
            android:text="@string/cl_dialog_daily_summary_not_data"
            android:textSize="@dimen/margin_14dp"/>
    </LinearLayout>

    <ListView
        android:id="@+id/lsv_id_schedule"
        style="@style/cs_common_match_wrap"
        android:divider="@color/cs_common_divider"
        android:dividerHeight="1px"
        android:visibility="gone"/>
</LinearLayout>
