<LinearLayout
    style="@style/cs_common_match_wrap"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!--<include layout="@layout/view_horizontal_user_list"/>-->
    <trente.asia.calendar.commons.views.UserListLinearLayout
        android:id="@+id/lnr_id_user_list"
        style="@style/cs_common_match_wrap"
        android:background="@drawable/state_common_selectable_row"
        android:paddingBottom="@dimen/margin_10dp"
        android:paddingTop="@dimen/margin_10dp">
    </trente.asia.calendar.commons.views.UserListLinearLayout>


    <LinearLayout
        android:id="@+id/lnr_calendar_container"
        style="@style/cs_common_match_wrap"
        android:orientation="vertical"/>

    <com.github.ksoichiro.android.observablescrollview.ObservableListView
        android:id="@+id/lst_calendar_day"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/margin_16dp"
        android:paddingRight="@dimen/margin_16dp"/>

</LinearLayout>
