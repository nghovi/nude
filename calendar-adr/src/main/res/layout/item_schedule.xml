<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/cs_common_match_wrap"
    android:background="@color/cl_cell_bg_color"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/txt_item_schedule_time"
        style="@style/wl_common_text"
        android:layout_width="@dimen/margin_150dp"
        android:layout_marginBottom="@dimen/margin_10dp"
        android:layout_marginTop="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/txt_item_schedule_name"
        style="@style/wl_common_text"
        android:layout_marginBottom="@dimen/margin_10dp"
        android:layout_marginTop="@dimen/margin_10dp"
        android:gravity="center_vertical" />


</LinearLayout>
